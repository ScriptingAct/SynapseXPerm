	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/ScriptingAct/SynxExplorer/main/Synx%20Dex'))()
  	end    
})

Tab:AddButton({
	Name = "inf Yield",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
  	end    
})


local Tab = Window:MakeTab({
	Name = "Setting",
